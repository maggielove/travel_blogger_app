<div class="form">
  <%= form_tag '/login' do %>

  <div id="login-labels">
    <p>
      <%= label_tag :username %>
      <%= text_field_tag :username %>
    </p>
    <p>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </p>
  </div>

    <%= submit_tag "Log in" %>

  <% end %>
</div>
